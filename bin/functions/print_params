#!/bin/bash
##############################################################################
###       输出参数详情
##############################################################################
function print_params(){
    info "REMOTE SERVER INFO"
    info "------------------------------------------------------------------------"
    info "remote_server_paths = ${remote_server_paths[@]}"
    info "remote_users = ${remote_users[@]}"
    info "remote_ips = ${remote_ips[@]}"
    info "remote_ports = ${remote_ports[@]}"
    info "remote_pwds = ${remote_pwds[@]}"
    info "remote_profiles = ${remote_profiles[@]}"
    info "remote_server_flags = ${remote_server_flags[@]}"
    info "remote_shell_dir = ${remote_shell_dir}"
    info "remote_shell_name = ${remote_shell_name}"
    info "remote_max_backup_file_num = ${remote_max_backup_file_num}"
    info "remote_backup_switch = ${remote_backup_switch}"
    info "------------------------------------------------------------------------"
    info "LOCAL SERVER INFO"
    info "------------------------------------------------------------------------"
    info "local_server_path = $local_server_path"
    info "------------------------------------------------------------------------"
    info "PROJECT INFO"
    info "------------------------------------------------------------------------"
    info "remote_project_basepath = $remote_project_basepath"
    info "local_project_basepath = $local_project_basepath"
    info "project_name = $project_name"
    info "war_sub_project_name = $war_sub_project_name"
    info "war_name = $war_name"
    info "local_profile = $local_profile"
    info "------------------------------------------------------------------------"
    info "CHECKOUT_COMMAND INFO"
    info "------------------------------------------------------------------------"
    info "checkout_command = ${checkout_command[@]}"
    info "------------------------------------------------------------------------"
    info "MAVEN INFO"
    info "------------------------------------------------------------------------"
    info "maven_home = $maven_home"
    info "------------------------------------------------------------------------"
    info "REPOSITORY INFO"
    info "------------------------------------------------------------------------"
    info "repository_url = $repository_url"
    info "------------------------------------------------------------------------"
    info "BUILD INFO"
    info "------------------------------------------------------------------------"
    info "parent_project_path = $parent_project_path"
    info "sub_project_path = $sub_project_path"    
    info "war_path = $war_path"
    info "profile = $profile"
    info "server_flag = $server_flag" 
    info "server_path = $server_path" 
    info "local_or_remote = $local_or_remote" 
    info "dirct_upload = $dirct_upload" 
    # info "clean_project = $clean_project" 
    # info "help_flag = $help_flag" 
    info "maven_shell = $maven_shell"
    info "backup_version = $backup_version"
    info "remote_server_path = $remote_server_path"
    info "remote_backup_path = $remote_backup_path"
    info "remote_user = $remote_user" 
    info "remote_ip = $remote_ip" 
    info "remote_port = $remote_port" 
    info "remote_pwd = $remote_pwd" 
    info "------------------------------------------------------------------------"
    info ""
}