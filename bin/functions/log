#!/bin/bash
##############################################################################
###       用于实现不同日志级别，用不同颜色表示不同的日志级别
##############################################################################
function error(){
    echo -e "\033[1;31m[ERROR] $@\033[m" 1>&2
}
function info(){
    echo -e "\033[1;32m[INFO] $@\033[0m"
}
function print(){
    echo -e "\033[1;32m$@\033[0m"
}
function warning(){
    echo -e "\033[1;33m[WARNING] $@\033[0m"
}
function success(){
    echo -e "\033[1;35m[SUCCESS] $@\033[0m"
}
function list(){
    echo -e "\033[0;37m$@\033[0m"
}
